{
    "manifest_version": 3,
    "name": "Media Speed Controller",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ],
    "permissions": [
        "tabs",
        "activeTab",
        "scripting",
        "storage"
    ]
}